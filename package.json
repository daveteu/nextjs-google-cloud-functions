  "bugs": {
    "url": "https://github.com/daveteu/nextjs-google-cloud-functions/issues"
  },
  "homepage": "https://github.com/daveteu/nextjs-google-cloud-functions#readme",
  "dependencies": {
    "express": "^4.17.1",
    "next": "^9.4.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
